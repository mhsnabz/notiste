<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@+id/progresBackground">
        <shape>
                <gradient android:startColor="#fff"
                    android:centerY="1.0"
                    android:endColor="#fff"
                    ></gradient>
            <stroke android:color="@color/colorPrimaryDark" android:width="1dp"></stroke>
            <corners android:radius="10dp"></corners>
        </shape>
    </item>
    <item android:id="@+id/progress">
        <clip>
            <shape>
                <gradient android:startColor="@color/colorPrimaryDark" android:endColor="@color/colorPrimaryDark"></gradient>
                <corners android:radius="10dp"></corners>
            </shape>
        </clip>
    </item>
</layer-list>